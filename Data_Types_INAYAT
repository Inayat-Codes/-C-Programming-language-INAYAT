  //-----------------------//
  int age = 25;
  printf("Age: %d\n",age);
  //-----------------------//

  float salary = 55000.50;
  printf("The salary is : %.2f\n",salary);
  //-----------------------//

  //char Emp_Name = 'Inayat';
  char Emp_Name[] = "Inayat";
